
@article{__2009,
  title = {{基于支持向量机的降雨\textemdash{}径流预测研究}},
  author = {汪, 丽娜 and 李, 粤安 and 陈, 晓宏},
  year = {2009},
  pages = {13--16},
  issn = {1000-0852},
  abstract = {本文探讨了支持向量机方法在降雨-径流预测中的应用。该方法采用结构风险最小化准则,弥补了人工神经网络在预测应用中的不足,较好地解决了小样本,非线性、高维数和局部最小点等实际问题。并且本研究通过与人工神经网络预测方法、传统的回归分析预测方法比较研究,得出支持向量机能取得更高精度的降雨-径流预测值。},
  journal = {水文},
  lccn = {11-1814/P},
  number = {01}
}


