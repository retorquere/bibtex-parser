}

@book{Anonymous:2011ci,
  title = {{{SandCanvas}}: A Multi-Touch Art Medium Inspired by Sand Animation},
  author = {Kazi, Rubaiat Habib and Chua, Kien Chuan and Zhao, Shengdong and Davis, Richard and Low, Kok-Lim},
  year = {2011},
  month = may,
