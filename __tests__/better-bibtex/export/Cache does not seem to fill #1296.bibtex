@article{Savidis:2002p7712,
  title = {Continuity of Interaction in Nomadic Interfaces through Migration and Dynamic Utilization of {{I}}/{{O}} Resources},
  author = {Savidis, A and Maou, N and Pachoulakis, I and Stephanidis, C},
  year = {2002},
  volume = {1},
  pages = {274--287},
  abstract = {The concept of ubiquitous computing reflects an infrastructure in which users are engaged in mobile interaction sessions within environments composed of dynamically varying computational resources. In this paradigm, applications are required to continuously follow end users and provide high-quality interaction while migrating among different computing devices and dynamically utilizing the available input/output (I/O) resources of each device. In the context of such interaction scenarios, the principle of continuity, which emphasizes the uninterrupted sequence of dialogue activities, is put forward as a key design goal. This paper presents an application experiment demonstrating interface migration, distributed I/O control, and dynamic I/O reconfiguration. The adopted dialogue design approach is discussed, along with the identified interaction design requirements, from the perspective of dialogue continuity. Finally, the employed software engineering strategy is presented, elaborating on the way dialogue mobility, distribution, and dynamic I/O control have been accomplished.},
  date-added = {2010-07-22T15:44:23GMT},
