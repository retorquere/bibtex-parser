  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/1998/Hummels/Automatic\%20Face\%20and\%20Gesture\%20Recognition\%201998.\%20Proceedings.\%20Third\%20IEEE\%20International\%20Conference\%20on\%201998\%20Hummels.pdf},
