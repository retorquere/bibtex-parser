@article{Anonymous:2011gw,
  title = {Bodily {{Orientations}} around {{Mobiles}}: {{Lessons}} Learnt in {{Vanuatu}}},
  year = {2011},
  pages = {277--286},
  doi = {10.1145/1978942.1978981},
  abstract = {Since we started carrying mobiles phones, they have altered the ways in which we orient our bodies in the world. Many of those changes are invisible to us \textendash{} they have become habits, deeply engrained in our society. To make us more aware of our bodily ways of living with mobiles and open the design space for novel ways of designing mobiles and their interactions, we decided to study one of the last groups of users on earth who had not been exposed to mobiles: the people of Vanuatu. As they had so recently started using mobiles, their use was still in flux: the fragility of the mobile was unusual to them as was the need to move in order to find coverage. They were still getting used to carrying their mobiles and keeping them safe. Their encounters with mobile use exposed the need to consider somaesthetics practices when designing mobiles as they profoundly affect our bodily ways of being in the world.},
