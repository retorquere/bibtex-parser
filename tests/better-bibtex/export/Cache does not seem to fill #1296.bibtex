  month = jan,
  number = {1},
  pages = {1-20},
  rating = {0},
  read = {Yes},
  title = {Indexing {{Multidimensional Time}}-{{Series}}},
  uri = {papers3://publication/doi/10.1007/s00778-004-0144-2},
  volume = {15},
