  keywords = {classification,geographic application,GPS,machine learning,transportation mode},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2008/Zheng/WWW\%20'08\%20Proceeding\%20of\%20the\%2017th\%20international\%20conference\%20on\%20World\%20Wide\%20Web\%202008\%20Zheng.pdf},
  pages = {247},
  publisher = {{ACM Press}},
  rating = {0},
  read = {Yes},
