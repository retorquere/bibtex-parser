@article{Terry:2011cl,
  abstract = {People routinely rely on Internet search engines to support their use of interactive systems: they issue queries to learn how to accomplish tasks, troubleshoot problems, and otherwise educate themselves on products. Given this common behavior, we argue that search query logs can usefully augment traditional usability methods by revealing the primary tasks and needs of a product's user population. We term this use of search query logs CUTS \textendash{} characterizing usability through search. In this paper, we introduce CUTS and describe an automated process for harvesting, ordering, labeling, filtering, and grouping search queries related to a given product. Importantly, this data set can be assembled in minutes, is timely, has a high degree of ecological validity, and is arguably less prone to self-selection bias than data gathered via traditional usability methods. We demonstrate the utility of this approach by applying it to a number of popular software and hardware systems.},
  author = {Terry, Michael},
  date-added = {2011-05-22T16:14:48GMT},
  date-modified = {2019-01-31T14:12:55GMT},
  doi = {10.1145/1978942.1979205},
  file = {files/Terry\:2011cl/SIGCHI Conference on Human-Computer Interaction 2011 Terry.pdf},
  journal = {SIGCHI Conference on Human-Computer Interaction},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Terry/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011\%20Terry.pdf},
  pages = {1817-1826},
  rating = {0},
  title = {Characterizing the {{Usability}} of {{Interactive Applications Through Query Log Analysis}}},
  uri = {papers3://publication/doi/10.1145/1978942.1979205},
  year = {2011}
}

