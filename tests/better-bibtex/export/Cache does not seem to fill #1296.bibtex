  month = feb,
  number = {1-2},
  pages = {17-25},
  rating = {0},
  title = {Gestural Interaction in the Pervasive Computing Landscape},
  uri = {papers3://publication/doi/10.1007/s00502-006-0413-4},
  volume = {124},
  year = {2007}
}

