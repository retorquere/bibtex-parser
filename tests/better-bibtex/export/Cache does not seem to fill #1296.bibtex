  month = oct,
  rating = {0},
  read = {Yes},
  title = {Distributed Pointing for Multimodal Collaboration over Sketched Diagrams},
  uri = {papers3://publication/uuid/6BAB8C22-05D8-4C8A-9744-89B8BB03E233},
