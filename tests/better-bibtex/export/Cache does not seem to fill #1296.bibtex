  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Unknown/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011-43.pdf},
  pages = {1423-1432},
  rating = {0},
  title = {Folk {{Music Goes Digital}} in {{India}}},
