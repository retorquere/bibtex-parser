  journal = {Wireless Symposium (IWS), 2013 IEEE International},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2013/Dementyev/2013\%20Dementyev.pdf},
  pages = {1-4},
  publisher = {{IEEE}},
  rating = {0},
  read = {Yes},
  title = {Power Consumption Analysis of {{Bluetooth Low Energy}}, {{ZigBee}} and {{ANT}} Sensor Nodes in a Cyclic Sleep Scenario},
  uri = {papers3://publication/doi/10.1109/IEEE-IWS.2013.6616827},
  year = {2013}
