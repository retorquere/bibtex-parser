@article{Rogers:2006p4976,
  abstract = {A motivation behind much UbiComp research has been to make our lives convenient, comfortable and informed, following in the footsteps of Weisers calm computing vision. Three themes that have dominated are context awareness, ambient intelligence and monitoring/tracking. While these avenues of research have been fruitful their accomplishments do not match up to anything like Weisers world. This paper discusses why this is so and argues that is time for a change of direction in the field. An alternative agenda is outlined that focuses on engaging rather than calming people. Humans are very resourceful at exploiting their environments and extending their capabilities using existing strategies and tools. I describe how pervasive technologies can be added to the mix, outlining three areas of practice where there is much potential for professionals and laypeople alike to combine, adapt and use them in creative and constructive ways.},
  author = {Rogers, Yvonne},
  date-added = {2009-06-01T12:50:34GMT},
  date-modified = {2015-07-30T20:04:03GMT},
  doi = {10.1007/11853565},
  file = {files/Rogers\:2006p4976/8th International Conference on Ubiquitous Computing (UbiComp) 2006 Rogers.pdf},
  journal = {8th International Conference on Ubiquitous Computing (UbiComp)},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2006/Rogers/8th\%20International\%20Conference\%20on\%20Ubiquitous\%20Computing\%20(UbiComp)\%202006\%20Rogers.pdf},
  pages = {404-421},
  rating = {0},
  title = {Moving on from {{Weisers Vision}} of {{Calm Computing}}: {{Engaging UbiComp Experiences}}},
  uri = {papers3://publication/doi/10.1007/11853565},
