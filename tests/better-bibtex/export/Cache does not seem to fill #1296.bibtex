  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/1998/Healey/International\%20Symposium\%20on\%20Wearable\%20Computers\%201998\%20Healey.pdf},
  pages = {42-49},
  rating = {0},
  read = {Yes},
  title = {{{StartleCam}}: A Cybernetic Wearable Camera},
