  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Kleek/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011\%20Kleek.pdf},
  month = may,
  publisher = {{ACM}},
  rating = {0},
  series = {A Longitudinal Study of People Managing Information Scraps in a Micro-Note Tool},
  title = {Finders/Keepers: A Longitudinal Study of People Managing Information Scraps in a Micro-Note Tool},
