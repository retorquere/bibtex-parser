  journal = {SIGCHI Conference on Human-Computer Interaction},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Oulasvirta/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011\%20Oulasvirta.pdf},
  pages = {3103-3112},
  rating = {0},
  title = {Ease of {{Juggling}}: {{Studying}} the {{Effects}} of {{Manual Multitasking}}},
