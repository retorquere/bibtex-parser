  isbn = {978-1-4503-0565-5},
  pages = {214--214},
  publisher = {{ACM}},
  series = {{{I3D}} '11},
  title = {Supporting {{Internal Visualization}} of {{Biomedical Datasets}} via {{3D Rapid Prototypes}} and {{Sketch}}-Based {{Gestures}}},
  year = {2011}
}

