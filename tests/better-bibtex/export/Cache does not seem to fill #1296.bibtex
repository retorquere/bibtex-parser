@article{Takahashi:1991p704,
  abstract = {It would be ideal for computer-human interaction if a computer could understand human gestures. Hand gestures are one means of interaction between computers and humans[1][2]. A hand gesture interface device, the VPL Data GloveTM, provides real-time information on a user's hand movement[3]. We propose a static hand gesture coding based on experiments and real-time hand gesture recognition method. Taking the 46 gestures of the Japanese kana manual alphabet as examples, we experimented to determine how many static hand configurations can be recognized both with off-line data and with real-time input from a Data GloveTM. In the experiment using off-line data, 23 of the gestures are distinguished and the other 23 figures are placed into 11 groups by coding hand configurations according to joint angles and hand orientation. Real-time hand gesture recognition experiments show that 34 of the 46 hand gestures are recognized correctly.},
  author = {Takahashi, Tomoichi},
  date-added = {2008-05-14T17:00:12GMT},
