  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Unknown/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011-38.pdf},
  pages = {287-296},
  rating = {0},
  title = {We {{Want More}}: {{Human}}-{{Computer Collaboration}} in {{Mobile Social Video Remixing}} of {{Music Concerts}}},
