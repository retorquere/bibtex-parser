  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Bateman/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011\%20Bateman.pdf},
  pages = {2355-2364},
  rating = {0},
  read = {Yes},
  title = {Target {{Assistance}} for {{Subtly Balancing Competitive Play}}},
