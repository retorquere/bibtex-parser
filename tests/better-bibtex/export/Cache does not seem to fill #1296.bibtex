@article{Lo:2014fo,
  abstract = {Deformable user interfaces leverage the physical actions we use intuitively to interact with real objects. It is therefore important to consider a prototype's physical characteristics when conducting research on deformable interactions. To create an authentic product experience, the authors set out to build a prototype that would mimic potential commercialized flexible devices and achieve the realism often lacking in low-fidelity prototypes. In this article, they outline the design and fabrication process to create Bendy, a prototype for studying deformable user interfaces. Their method creates prototypes quickly (one day) and inexpensively (approximately US 70) by using readily available materials. In addition, the process lets other researchers customize physical properties and interaction language to fit their specific purposes. The deformable prototype is composed of three main layers: a flexible plastic, an array of bend sensors connected to a flexible circuit, and a layer of silicone that encloses the sensors and circuit. The authors describe the fabrication process and demonstrate their method with two additional case studies. This article is part of a special issue on fabrication and printing. The Web extra shows the fabrication process, from the circuit design to printing and testing our flexible circuit, to creating the final Bendy prototype and playing Pac-Man using our deformable prototype. The Web extra for this article can be found at http://youtu.be/PJ5ee5gAbm8. View full abstract},
  author = {Lo, J and Girouard, A},
  date-added = {2014-07-23T00:13:21GMT},
  date-modified = {2019-01-31T14:13:25GMT},
  doi = {10.1109/MPRV.2014.47},
  journal = {Pervasive Computing, IEEE},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/Unknown/Lo/IEEE\%20Pervasive\%20Comput.\%20\%20Lo.pdf},
  number = {3},
  pages = {40-46},
  rating = {0},
  title = {Fabricating {{Bendy}}: {{Design}} and {{Development}} of {{Deformable Prototypes}}},
  uri = {papers3://publication/doi/10.1109/MPRV.2014.47},
  volume = {13},
  year = {2014}
}

