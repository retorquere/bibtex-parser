  isbn = {978-1-4503-4922-2},
  keywords = {big questions,cohesion,disciplinarity,HCI,intellectual progress,research questions,status},
  pages = {969--981},
  publisher = {{ACM}},
  series = {{{DIS}} '17},
  shorttitle = {Reviewing the {{Big Questions Literature}};},
  title = {Reviewing the {{Big Questions Literature}};: {{Or}}, {{Should HCI Have Big Questions}}?},
  year = {2017}
}

