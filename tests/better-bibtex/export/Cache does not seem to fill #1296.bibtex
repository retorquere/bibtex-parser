@article{Girouard:2011kl,
  abstract = {Flexible displays potentially allow for interaction styles that resemble those used in paper documents. Bending the display, e.g., to page forward, shows particular promise as an interaction technique. In this paper, we present an evaluation of the effectiveness of various bend gestures in executing a set of tasks with a flexible display. We discuss a study in which users designed bend gestures for common computing actions deployed on a smartphone-inspired flexible E Ink prototype called PaperPhone. We collected a total of 87 bend gesture pairs from ten participants and their appropriateness over twenty actions in five applications. We identi- fied six most frequently used bend gesture pairs out of 24 unique pairs. Results show users preferred bend gestures and bend gesture pairs that were conceptually simpler, e.g., along one axis, and less physically demanding. There was a strong agreement among participants to use the same three pairs in applications: (1) side of display, up/down (2) top corner, up/down (3) bottom corner, up/down. For actions with a strong directional cue, we found strong consensus on the polarity of the bend gestures (e.g., navigating left is performed with an upwards bend gesture, navigating right, downwards). This implies that bend gestures that take directional cues into account are likely more natural to users.},
  author = {Girouard, Audrey},
  date-added = {2011-05-22T16:14:47GMT},
  date-modified = {2019-01-31T14:12:56GMT},
  doi = {10.1145/1978942.1979136},
  file = {files/Girouard\:2011kl/SIGCHI Conference on Human-Computer Interaction 2011 Girouard.pdf},
  journal = {SIGCHI Conference on Human-Computer Interaction},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Girouard/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011\%20Girouard.pdf},
  pages = {1303-1312},
  rating = {0},
  title = {{{PaperPhone}}: {{Understanding}} the {{Use}} of {{Bend Gestures}} in {{Mobile Devices}} with {{Flexible Electronic Paper Displays}}},
  uri = {papers3://publication/doi/10.1145/1978942.1979136},
  year = {2011}
}

