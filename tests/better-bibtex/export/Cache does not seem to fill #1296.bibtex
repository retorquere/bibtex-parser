@inproceedings{Kohlsdorf:ey,
  abstract = {False positives are a common problem for interfaces that rely on gesture recognition. Often a gesture can seem fine in development but is found to trigger accidentally during an initial deployment of the interface, restarting development and increasing expense. In this work we introduce MAGIC 2.0, a technique for false positive prediction and prevention that can be used interactively during the interface design process. To ground our research, we implement MAGIC 2.0 as a web service and develop gesture interfaces using sensors on common Android mobile phone platforms. We use iSAX (indexable Symbolic Aggregate approXimation) to enable interactive searching (\textexclamdown;2 sec/example) of a large database (\textquestiondown;1,500,000 sec) of everyday user movements on a standard workstation to determine if a candidate gesture will trigger accidentally during use of an interface. We perform a user-independent study that suggests that the number of matches to this Everyday Gesture Library (EGL) database is indeed predictive of a candidate gesture's suitability. We compare iSAX to hidden Markov models (HMMs) and nearest neighbor with respect to accuracy and speed for the EGL search. Using iSAX on the EGL, we also develop a garbage class and show that including this class in recognition reduces errors. View full abstract},
  author = {Kohlsdorf, D and Starner, T and Ashbrook, Daniel},
  booktitle = {Automatic Face {{\&}} Gesture Recognition and Workshops ({{FG}} 2011), 2011 {{IEEE}} International Conference On},
  date-added = {2013-10-21T03:47:32GMT},
  date-modified = {2019-01-31T14:13:20GMT},
  doi = {10.1109/FG.2011.5771412},
  file = {files/Kohlsdorf\:ey/Gesture Recognition (FG 2011) 2011 Kohlsdorf.pdf},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Kohlsdorf/Gesture\%20Recognition\%20(FG\%202011)\%202011\%20Kohlsdorf.pdf},
  pages = {1-6},
  rating = {0},
  read = {Yes},
  title = {{{MAGIC}} 2.0: {{A}} Web Tool for False Positive Prediction and Prevention for Gesture Recognition Systems},
  uri = {papers3://publication/doi/10.1109/FG.2011.5771412},
  year = {2011}
}

