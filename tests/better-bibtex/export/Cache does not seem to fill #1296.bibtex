  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2002/Savidis/Universal\%20Access\%20in\%20the\%20Information\%20Society\%202002\%20Savidis.pdf},
  number = {4},
  pages = {274-287},
  rating = {0},
  read = {Yes},
  title = {Continuity of Interaction in Nomadic Interfaces through Migration and Dynamic Utilization of {{I}}/{{O}} Resources},
