@article{Sung:2010p8776,
  abstract = {With the help of virtualization and cloud technologies [1, 2], users desktop service can be made available on internet or local area network. This service is called virtual desktop or desktop cloud. However, user interface is limited to I/O capabilities of a users mobile terminal. In order to enhance user interface in remote virtualized desktop, it is important to take advantage of fullfeatured I/O devices found locally. Our previous work called SoD (System-onDemand) [3, 4] proposed how to associate local I/O devices with remote virtualized desktop in Xen [5]. It is required to install SoD client agent in a users mobile terminal. In this paper, we propose a new framework called WebSoD that does not require any explicit installation to make SoD service available in virtual desktop. In the Web-SoD, the SoD service is provided by the web technology so that the SoD client agent can be installed transparently, and platform independency is also achieved. For this purpose, we extend Native Client (NaCl) proposed by Google [6] to support networking by modifying NaCl compiler, library and service runtime. By the proposed Web-SoD framework, it is possible for local I/O devices to be available remotely via Web interface \textendash{} transforming local legacy I/O devices into network-wide I/O devices. Then, a user can dynamically configure the network-wide I/O devices to the virtual desktop in order to get better user interface. In addition, it is possible to eliminate an explicit installation of complex agents.},
  author = {Sung, Baegjae and Park, Sejin and Park, Chanik},
  date-added = {2010-11-29T20:36:05GMT},
  date-modified = {2015-07-30T20:04:03GMT},
  file = {files/Sung\:2010p8776/Wearable Computers 2010. Proceedings. Thirteenth IEEE International Symposium on 2010 Sung.pdf},
  journal = {Wearable Computers, 2010. Proceedings. Thirteenth IEEE International Symposium on},
  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2010/Sung/Wearable\%20Computers\%202010.\%20Proceedings.\%20Thirteenth\%20IEEE\%20International\%20Symposium\%20on\%202010\%20Sung.pdf},
  pages = {253-256},
  rating = {0},
  read = {Yes},
  title = {Web-{{SoD}}: {{Supporting Web}}-Based {{I}}/{{O Services}} in {{Xen Environment}} by {{Native Client}} with {{Network Extension}}},
  uri = {papers3://publication/uuid/D26AF823-F596-4390-B768-F69EBD7B11FA},
  year = {2010}
}

