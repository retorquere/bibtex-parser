  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Unknown/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011-10.pdf},
  pages = {763-772},
  rating = {0},
  title = {Starcraft from the {{Stands}}: {{Understanding}} the {{Game Spectator}}},
