  number = {3},
  pages = {372-381},
  publisher = {{IEEE}},
  rating = {0},
  read = {Yes},
  title = {Strain Sensing Fabric for Hand Posture and Gesture Monitoring},
  uri = {papers3://publication/doi/10.1109/TITB.2005.854510},
  volume = {9},
  year = {2005}
}

