  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2008/Baudisch/AVI\%20'08\%20Proceedings\%20of\%20the\%20working\%20conference\%20on\%20Advanced\%20visual\%20interfaces\%202008\%20Baudisch.pdf},
  month = may,
  rating = {0},
  title = {Starburst: A Target Expansion Algorithm for Non-Uniform Target Distributions},
  uri = {papers3://publication/uuid/3E121140-0BB8-4883-B4A7-4C34DC5D7CA9},
  year = {2008}
}

