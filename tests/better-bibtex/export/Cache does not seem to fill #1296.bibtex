  local-url = {file://localhost/Users/dan/Dropbox/Apps/Library.papers3/Articles/2011/Cockburn/SIGCHI\%20Conference\%20on\%20Human-Computer\%20Interaction\%202011\%20Cockburn.pdf},
  pages = {2741-2750},
  rating = {0},
  title = {Dips and {{Ceilings}}: {{Understanding}} and {{Supporting Transitions}} to {{Expertise}} in {{User Interfaces}}},
