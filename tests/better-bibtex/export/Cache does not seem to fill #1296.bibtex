@article{Fourie:2005p1461,
  abstract = {The aim of this study was to compare how efficiently a normally hearing adult could learn sign language vocabulary from a teacher versus three types of media, namely a CD-ROM, a video and a book. A single-subject, time-series, control-versus-multiple-treatments design was used. Expressive and receptive learning curves for each method using regression analysis were plotted and then analysed statistically. Results suggested that the subject learned sign language vocabulary most efficiently from the teacher followed closely by the video, then the CD-ROM, and then the book. These differences were probably due to the feedback and personal contact provided only by the teacher, as well as the greater clarity of the teacher's signs as opposed to the clarity of the media.},
  affiliation = {University of the Witwatersrand, Johannesburg, South Africa},
  author = {Fourie, Robert James},
