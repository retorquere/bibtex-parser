{
  "name": "@retorquere/bibtex-parser",
  "version": "3.2.30",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -t stylish --project .",
    "submodules": "git submodule update --init --recursive --remote && git submodule foreach git reset --hard && git submodule foreach git checkout master && git submodule foreach git pull origin master",
    "prestart": "pnpm run lint && pegjs grammar.pegjs && pnpm run submodules",
    "start": "tsc -d",
    "poststart": "ts-node build/parse-abbrevs.ts && pnpm run docs",
    "pretest": "pnpm start && ts-node gen-tests.ts",
    "update-ci": "ts-node gen-tests.ts && jest -u",
    "retest": "tsc -d && ts-node gen-tests.ts && jest",
    "test": "jest",
    "minitest": "pegjs grammar.pegjs && tsc -d && node i.js",
    "list": "tar tvf $(pnpm pack)",
    "prepublishOnly": "pnpm start",
    "preversion": "pnpm test",
    "postversion": "git push --follow-tags",
    "update-cases": "rsync -av --delete --include='merge/' --include='import/' --include='export/' --exclude='*/' --include='*.bib' --include='*.bibtex' --include='*.biblatex' --exclude='*' ../better-bibtex/test/fixtures/ __tests__/better-bibtex/",
    "update-snapshots": "jest --updateSnapshot",
    "docs": "typedoc --plugin typedoc-plugin-markdown --disableOutputCheck . && touch docs/.nojekyll",
    "ncu": "pnpm run submodules && git add jabref abbrv.jabref.org && ncu -u && pnpm i && git add package.json package-lock.json && git commit -m ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.20",
    "csv": "^5.3.2",
    "digital-tree": "^2.0.1",
    "jest": "^26.6.3",
    "jest-specific-snapshot": "^4.0.0",
    "markdown": "^0.5.0",
    "mkdirp": "^1.0.4",
    "pegjs": "^0.10.0",
    "slugify": "^1.4.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.12",
    "typedoc-plugin-markdown": "^3.3.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chevrotain": "^7.1.0",
    "unicode2latex": "^2.1.35",
    "xregexp": "^4.4.1"
  },
  "jest": {
    "coverageDirectory": "node_modules/.cache/coverage",
    "collectCoverageFrom": [
      "*.ts",
      "!*.d.ts"
    ],
    "reporters": [
      "default"
    ],
    "testSequencer": "./test-sequencer.js"
  },
  "files": [
    "package.json",
    "grammar.js",
    "grammar.d.ts",
    "chunker.d.ts",
    "chunker.js",
    "index.d.ts",
    "index.js",
    "jabref.d.ts",
    "jabref.js",
    "README.md",
    "strings.json",
    "unabbrev.json",
    "abbrev.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
