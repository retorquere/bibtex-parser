{
  "name": "@retorquere/bibtex-parser",
  "version": "9.0.18",
  "description": "BibTeX parser",
  "main": "index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "unified": "tsx unified.ts",
    "lint": "dprint fmt *.ts && dprint check *.ts",
    "prestart": "npm run lint",
    "start": "npm run tsc",
    "tsc": "tsc -d",
    "pretest": "npm start",
    "test": "tap --timeout 0 --disable-coverage --allow-empty-coverage",
    "snap": "npm test --snapshot",
    "snap:big": "npm test --snapshot --big",
    "minitest": "./build/crossref.py && tsc -d && node i.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "abbr": "./build/abbr.py",
    "preversion": "./console.py r && npm run abbr && npm run test",
    "postversion": "git push --follow-tags",
    "update-cases": "curl -s https://raw.githubusercontent.com/retorquere/zotero-better-bibtex/master/test/loaded.json | jq -r '.[] | select(test(\"\\\\.bib((la)?tex)?$\"))' | rsync -av --delete -m --files-from=- ../better-bibtex/test/fixtures/ test/better-bibtex/",
    "predocs": "npm start",
    "docs": "typedoc",
    "ncu": "./build/submodules.py && ./build/crossref.py && ncu -u && npm i && git add package.json package-lock.json && npm audit fix && git commit -m ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "dprint": "^0.50.0",
    "@types/node": "^22.15.29",
    "command-line-args": "^6.0.1",
    "csv": "^6.3.11",
    "digital-tree": "^2.0.4",
    "ejs": "^3.1.10",
    "esbuild": "^0.25.5",
    "glob": "^11.0.2",
    "mkdirp": "^3.0.1",
    "node-fetch": "^3.3.2",
    "piscina": "^5.0.0",
    "slug": "^11.0.0",
    "tap": "^21.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@unified-latex/unified-latex-util-pegjs": "^1.8.1",
    "@unified-latex/unified-latex-util-print-raw": "^1.8.0",
    "@unified-latex/unified-latex-util-replace": "^1.8.2",
    "@unified-latex/unified-latex-util-visit": "^1.8.2",
    "i": "^0.3.7",
    "lodash.merge": "^4.6.2",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "unicode2latex": "^6.0.13",
    "wink-eng-lite-web-model": "^1.8.1",
    "wink-nlp": "^2.3.2",
    "xregexp": "^5.1.2"
  },
  "files": [
    "README.md",
    "crossref.json",
    "fields.json",
    "index.d.ts",
    "index.js",
    "index.js.map",
    "jabref.d.ts",
    "jabref.js",
    "jabref.js.map",
    "merge.d.ts",
    "merge.js",
    "merge.js.map",
    "package.json",
    "prepositions.json",
    "re.d.ts",
    "re.js",
    "re.js.map",
    "sentence-case.d.ts",
    "sentence-case.js",
    "sentence-case.js.map",
    "strings.bib",
    "tokenizer.d.ts",
    "tokenizer.js",
    "tokenizer.js.map",
    "unabbrev.json",
    "verbatim.d.ts",
    "verbatim.js",
    "verbatim.js.map",
    "yield.d.ts",
    "yield.js",
    "yield.js.map"
  ],
  "publishConfig": {
    "access": "public"
  }
}
