{
  "name": "@retorquere/bibtex-parser",
  "version": "3.0.7",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -t stylish --project .",
    "prestart": "npm run lint && pegjs grammar.pegjs",
    "start": "tsc -d",
    "poststart": "npm run docs",
    "pretest": "npm start",
    "test": "jest",
    "minitest": "pegjs grammar.pegjs && tsc -d && node tt.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "preversion": "npm test",
    "postversion": "git push --follow-tags",
    "update-cases": "rsync -av --delete --include='import/' --include='export/' --exclude='*/' --include='*.bib' --include='*.bibtex' --include='*.biblatex' --exclude='*' ../better-bibtex/test/fixtures/ tests/better-bibtex/",
    "update-snapshots": "jest --updateSnapshot",
    "docs": "typedoc --disableOutputCheck . && touch docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@retorquere/jasmine-fail-fast": "^2.0.4",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "jest": "^24.9.0",
    "jest-progress-tracker": "^2.0.2",
    "jest-specific-snapshot": "^2.0.0",
    "jsesc": "^2.5.2",
    "pegjs": "^0.10.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.4",
    "unicode2latex": "^1.0.33"
  },
  "dependencies": {
    "xregexp": "^4.2.4"
  },
  "jest": {
    "coverageDirectory": "node_modules/.cache/coverage",
    "collectCoverageFrom": [
      "*.ts",
      "!*.d.ts"
    ],
    "preset": "ts-jest",
    "reporters": [
      "default",
      "jest-progress-tracker"
    ]
  },
  "files": [
    "package.json",
    "grammar.js",
    "grammar.d.ts",
    "chunker.d.ts",
    "chunker.js",
    "index.d.ts",
    "index.js",
    "jabref.d.ts",
    "jabref.js",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
