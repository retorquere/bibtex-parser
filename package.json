{
  "name": "bibtex-parser",
  "version": "1.0.0",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "_pregrammar": "curl -OL https://raw.githubusercontent.com/retorquere/astrocite/master/packages/astrocite-bibtex/src/grammar.pegjs",
    "pregrammar": "cp /Users/emile/github/astrocite/packages/astrocite-bibtex/src/grammar.pegjs .",
    "grammar": "pegjs grammar.pegjs",
    "prestart": "npm run grammar",
    "start": "tsc *.ts",
    "poststart": "node unicode2latex.js",
    "pretest": "npm start",
    "test": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/retorquere"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "jsesc": "^2.5.2",
    "pegjs": "^0.10.0",
    "typescript": "^3.5.3",
    "unicode2latex": "^1.0.19"
  },
  "dependencies": {
    "pegjs-backtrace": "^0.2.0"
  }
}
