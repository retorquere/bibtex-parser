{
  "name": "@retorquere/bibtex-parser",
  "version": "9.0.23",
  "description": "BibTeX parser",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "typings": "./index.d.ts",
  "scripts": {
    "unified": "tsx unified.ts",
    "lint": "dprint fmt *.ts && dprint check *.ts",
    "prestart": "npm run lint",
    "start": "npm run build",
    "build:esm": "rm -rf dist/esm && esbuild *.ts --outdir=dist/esm --format=esm --sourcemap --target=es2020 --loader:.ts=ts",
    "build:cjs": "rm -rf dist/cjs && esbuild *.ts --outdir=dist/cjs --format=cjs --sourcemap --target=node16 --loader:.ts=ts",
    "build:types": "rm -rf dist/cjs dist/types && tsc -d -p tsconfig.cjs.json",
    "prebuild": "rm -rf dist && mkdir dist && ./build/abbr.py && ./build/crossref.py && ./build/prepositions.py",
    "build": "npm run build:types && npm run build:cjs && npm run build:esm",
    "pretest": "npm start",
    "test": "node test/run.js",
    "minitest": "./build/crossref.py && npm run build && node i.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "abbr": "./build/abbr.py",
    "preversion": "npm run abbr && npm run test",
    "postversion": "git push --follow-tags",
    "update-cases": "curl -s https://raw.githubusercontent.com/retorquere/zotero-better-bibtex/master/test/loaded.json | jq -r '.[] | select(test(\"\\\\.bib((la)?tex)?$\"))' | rsync -av --delete -m --files-from=- ../better-bibtex/test/fixtures/ test/better-bibtex/",
    "predocs": "npm start",
    "docs": "typedoc",
    "ncu": "./build/submodules.py && ./build/crossref.py && ncu -u && npm i && git add package.json package-lock.json && npm audit fix && git commit -m ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.9.0",
    "command-line-args": "^6.0.1",
    "csv": "^6.4.1",
    "digital-tree": "^2.0.4",
    "dprint": "^0.50.2",
    "ejs": "^3.1.10",
    "esbuild": "^0.25.11",
    "glob": "^11.0.3",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "node-fetch": "^3.3.2",
    "piscina": "^5.1.3",
    "slug": "^11.0.1",
    "tap": "^21.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@unified-latex/unified-latex-util-pegjs": "^1.8.1",
    "@unified-latex/unified-latex-util-print-raw": "^1.8.0",
    "@unified-latex/unified-latex-util-replace": "^1.8.3",
    "@unified-latex/unified-latex-util-visit": "^1.8.3",
    "i": "^0.3.7",
    "lodash.merge": "^4.6.2",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "unicode2latex": "^7.0.8",
    "wink-eng-lite-web-model": "^1.8.1",
    "wink-nlp": "^2.4.0"
  },
  "files": [
    "README.md",
    "package.json",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
