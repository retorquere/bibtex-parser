{
  "name": "@retorquere/bibtex-parser",
  "version": "1.0.68",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "fetch-grammar": "curl -L https://raw.githubusercontent.com/retorquere/astrocite/master/packages/astrocite-bibtex/src/grammar.pegjs -o astrocite-bibtex.pegjs",
    "copy-grammar": "cp /Users/emile/github/astrocite/packages/astrocite-bibtex/src/grammar.pegjs astrocite-bibtex.pegjs",
    "fetch-schema": "curl -L https://raw.githubusercontent.com/retorquere/astrocite/master/packages/astrocite-bibtex/src/schema.d.ts -o astrocite-bibtex.d.ts",
    "copy-schema": "cp /Users/emile/github/astrocite/packages/astrocite-bibtex/src/schema.d.ts astrocite-bibtex.d.ts",
    "pregrammar": "npm run fetch-schema && npm run fetch-grammar",
    "postgrammar": "cat astrocite-bibtex-d-ts >> astrocite-bibtex.d.ts",
    "edit-grammar": "vi /Users/emile/github/astrocite/packages/astrocite-bibtex/src/grammar.pegjs",
    "grammar": "pegjs astrocite-bibtex.pegjs",
    "lint": "tslint -t stylish --project .",
    "prestart": "npm run grammar && npm run lint",
    "start": "tsc -d",
    "poststart": "ts-node setup/unicode2latex.ts && npm run docs",
    "pretest": "npm start",
    "test": "jest",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "preversion": "npm test",
    "postversion": "git push --follow-tags && npm publish --access public",
    "update-cases": "rsync -avz --include='import/' --include='export/' --exclude='*/' --include='*.bib' --include='*.bibtex' --include='*.biblatex' --exclude='*' ../better-bibtex/test/fixtures/ tests/better-bibtex/",
    "update-snapshots": "jest --updateSnapshot",
    "docs": "typedoc ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "jest-specific-snapshot": "^2.0.0",
    "jsesc": "^2.5.2",
    "pegjs": "^0.10.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.1.4",
    "typescript": "^3.5.3",
    "unicode2latex": "^1.0.22"
  },
  "dependencies": {},
  "jest": {
    "coverageDirectory": "node_modules/.cache/coverage",
    "collectCoverageFrom": [
      "*.ts",
      "!*.d.ts"
    ],
    "preset": "ts-jest"
  }
}
