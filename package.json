{
  "name": "@retorquere/bibtex-parser",
  "version": "6.0.0",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "lint": "eslint . --ext .ts --cache --cache-location .eslintcache/",
    "prestart": "npm run lint && peggy grammar.peggy",
    "start": "tsc -d",
    "poststart": "npm run docs",
    "pretest": "npm start",
    "test": "node test/jest",
    "presnap": "npm start",
    "snap": "node test/jest --updateSnapshot",
    "minitest": "peggy grammar.peggy && tsc -d && node i.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "abbr": "./build/abbr.py",
    "preversion": "npm test && npm run abbr",
    "postversion": "git push --follow-tags",
    "update-cases": "curl -s https://raw.githubusercontent.com/retorquere/zotero-better-bibtex/master/test/loaded.json | jq -r '.[] | select(test(\"\\\\.bib((la)?tex)?$\"))' | rsync -av --delete -m --files-from=- ../better-bibtex/test/fixtures/ test/better-bibtex/",
    "docs": "rm -rf docs && typedoc --name 'BibTeX parser' index.ts && touch docs/.nojekyll",
    "prencu": "git submodule update --init --recursive --remote && git submodule foreach git reset --hard && git submodule foreach git checkout master && git submodule foreach git pull origin master && git add abbrv.jabref.org",
    "ncu": "ncu -u && npm i && git add package.json package-lock.json && git commit -m ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "command-line-args": "^5.2.0",
    "csv": "^6.0.4",
    "digital-tree": "^2.0.3",
    "ejs": "^3.1.6",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "jest-specific-snapshot": "^5.0.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.1.0",
    "peggy": "^1.2.0",
    "slug": "^5.1.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.9",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "unicode2latex": "^2.1.36",
    "xregexp": "^5.1.0"
  },
  "files": [
    "package.json",
    "grammar.js",
    "grammar.d.ts",
    "chunker.d.ts",
    "chunker.js",
    "index.d.ts",
    "index.js",
    "jabref.d.ts",
    "jabref.js",
    "README.md",
    "crossref.json",
    "strings.bib",
    "unabbrev.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
