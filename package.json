{
  "name": "@retorquere/bibtex-parser",
  "version": "6.3.4",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts --cache --cache-location .eslintcache/",
    "prestart": "npm run lint && peggy grammar.peggy",
    "start": "tsc -d",
    "poststart": "npm run docs",
    "pretest": "npm start",
    "test": "tap test/tap --no-timeout --no-coverage",
    "snap": "npm test --snapshot",
    "snap:big": "npm test --snapshot --big",
    "minitest": "peggy grammar.peggy && tsc -d && node i.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "abbr": "./build/abbr.py",
    "preversion": "npm test && npm run abbr",
    "postversion": "git push --follow-tags",
    "update-cases": "curl -s https://raw.githubusercontent.com/retorquere/zotero-better-bibtex/master/test/loaded.json | jq -r '.[] | select(test(\"\\\\.bib((la)?tex)?$\"))' | rsync -av --delete -m --files-from=- ../better-bibtex/test/fixtures/ test/better-bibtex/",
    "docs": "rm -rf docs && typedoc --name 'BibTeX parser' index.ts && touch docs/.nojekyll",
    "ncu": "./build/submodules.py && ncu -u && npm i && git add package.json package-lock.json && npm audit fix && git commit -m ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "command-line-args": "^5.2.1",
    "csv": "^6.0.5",
    "digital-tree": "^2.0.4",
    "ejs": "^3.1.7",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "glob": "^8.0.1",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.2.4",
    "peggy": "^1.2.0",
    "piscina": "^3.2.0",
    "slug": "^5.3.0",
    "tap": "^16.2.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "unicode2latex": "^3.0.0",
    "xregexp": "^5.1.0"
  },
  "files": [
    "package.json",
    "grammar.js",
    "grammar.d.ts",
    "chunker.d.ts",
    "chunker.js",
    "index.d.ts",
    "index.js",
    "jabref.d.ts",
    "jabref.js",
    "README.md",
    "crossref.json",
    "strings.bib",
    "unabbrev.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
