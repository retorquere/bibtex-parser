{
  "name": "@retorquere/bibtex-parser",
  "version": "3.1.26",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -t stylish --project .",
    "prestart": "npm run lint && pegjs grammar.pegjs && npm run submodules",
    "submodules": "git submodule update --init --recursive --remote && git submodule foreach git reset --hard && git submodule foreach git checkout master && git submodule foreach git pull origin master",
    "start": "tsc -d",
    "poststart": "ts-node build/parse-abbrevs.ts && npm run docs",
    "pretest": "npm start && ts-node gen-tests.ts",
    "update-ci": "ts-node gen-tests.ts && jest -u",
    "test": "jest",
    "minitest": "pegjs grammar.pegjs && tsc -d && node tt.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "preversion": "npm test",
    "postversion": "git push --follow-tags",
    "update-cases": "rsync -av --delete --include='merge/' --include='import/' --include='export/' --exclude='*/' --include='*.bib' --include='*.bibtex' --include='*.biblatex' --exclude='*' ../better-bibtex/test/fixtures/ __tests__/better-bibtex/",
    "update-snapshots": "jest --updateSnapshot",
    "docs": "typedoc --theme markdown --disableOutputCheck . && touch docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@retorquere/jasmine-fail-fast": "^2.0.4",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "csv": "^5.3.2",
    "digital-tree": "^2.0.1",
    "jest": "^25.1.0",
    "jest-progress-tracker": "^2.0.2",
    "jest-specific-snapshot": "^3.0.0",
    "jsesc": "^2.5.2",
    "markdown": "^0.5.0",
    "mkdirp": "^1.0.3",
    "pegjs": "^0.10.0",
    "slugify": "^1.4.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typedoc": "^0.16.11",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "xregexp": "^4.3.0",
    "unicode2latex": "^1.0.53"
  },
  "jest": {
    "coverageDirectory": "node_modules/.cache/coverage",
    "collectCoverageFrom": [
      "*.ts",
      "!*.d.ts"
    ],
    "reporters": [
      "default",
      "jest-progress-tracker"
    ],
    "testSequencer": "./test-sequencer.js"
  },
  "files": [
    "package.json",
    "grammar.js",
    "grammar.d.ts",
    "chunker.d.ts",
    "chunker.js",
    "index.d.ts",
    "index.js",
    "jabref.d.ts",
    "jabref.js",
    "README.md",
    "strings.json",
    "unabbrev.json",
    "abbrev.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
