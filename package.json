{
  "name": "@retorquere/bibtex-parser",
  "version": "5.2.4",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "lint": "eslint . --ext .ts --cache --cache-location .eslintcache/",
    "prestart": "npm run lint && pegjs grammar.pegjs",
    "start": "tsc -d",
    "poststart": "npm run docs",
    "pretest": "npm start",
    "test": "node test/jest",
    "presnap": "npm start",
    "snap": "node test/jest --updateSnapshot",
    "minitest": "pegjs grammar.pegjs && tsc -d && node i.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "abbr": "./build/abbr.py",
    "preversion": "npm test && npm run abbr",
    "postversion": "git push --follow-tags",
    "update-cases": "curl -s https://raw.githubusercontent.com/retorquere/zotero-better-bibtex/master/test/loaded.json | jq -r '.[] | select(test(\"\\\\.bib((la)?tex)?$\"))' | rsync -av --delete -m --files-from=- ../better-bibtex/test/fixtures/ test/better-bibtex/",
    "docs": "rm -rf docs && typedoc --name 'BibTeX parser' index.ts && touch docs/.nojekyll",
    "prencu": "git submodule update --init --recursive --remote && git submodule foreach git reset --hard && git submodule foreach git checkout master && git submodule foreach git pull origin master",
    "ncu": "ncu -u && npm i && git add package.json package-lock.json && git commit -m ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.4.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "command-line-args": "^5.2.0",
    "csv": "^5.5.0",
    "digital-tree": "^2.0.3",
    "ejs": "^3.1.6",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "jest-specific-snapshot": "^5.0.0",
    "markdown": "^0.5.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.1",
    "pegjs": "^0.10.0",
    "slug": "^5.1.0",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "unicode2latex": "^2.1.35",
    "xregexp": "^5.1.0"
  },
  "files": [
    "package.json",
    "grammar.js",
    "grammar.d.ts",
    "chunker.d.ts",
    "chunker.js",
    "index.d.ts",
    "index.js",
    "jabref.d.ts",
    "jabref.js",
    "README.md",
    "crossref.json",
    "strings.bib",
    "unabbrev.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
