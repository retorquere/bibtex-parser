{
  "name": "@retorquere/bibtex-parser",
  "version": "1.0.2",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "pregrammar": "curl -L https://raw.githubusercontent.com/retorquere/astrocite/master/packages/astrocite-bibtex/src/grammar.pegjs -o astrocite-bibtex.pegjs",
    "_pregrammar": "cp /Users/emile/github/astrocite/packages/astrocite-bibtex/src/grammar.pegjs .",
    "grammar": "pegjs astrocite-bibtex.pegjs",
    "lint": "tslint -t stylish --project .",
    "prestart": "npm run grammar && npm run lint",
    "start": "tsc *.ts",
    "poststart": "node unicode2latex.js",
    "pretest": "npm start",
    "test": "node test.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "go": "npm start && npm version patch && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "jsesc": "^2.5.2",
    "pegjs": "^0.10.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "unicode2latex": "^1.0.19"
  }
}
