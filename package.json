{
  "name": "@retorquere/bibtex-parser",
  "version": "3.3.2",
  "description": "BibTeX parser",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -t stylish --project .",
    "prestart": "npm run lint && pegjs grammar.pegjs",
    "start": "tsc -d",
    "poststart": "npm run docs",
    "pretest": "npm start && ts-node gen-tests.ts",
    "update-ci": "ts-node gen-tests.ts && jest -u",
    "retest": "tsc -d && ts-node gen-tests.ts && jest",
    "test": "jest",
    "minitest": "pegjs grammar.pegjs && tsc -d && node i.js",
    "list": "tar tvf $(npm pack)",
    "prepublishOnly": "npm start",
    "preversion": "npm test",
    "postversion": "git push --follow-tags",
    "update-cases": "rsync -av --delete --include='merge/' --include='import/' --include='export/' --exclude='*/' --include='*.bib' --include='*.bibtex' --include='*.biblatex' --exclude='*' ../better-bibtex/test/fixtures/ __tests__/better-bibtex/",
    "update-snapshots": "jest --updateSnapshot",
    "docs": "rm -rf docs && typedoc --name 'BibTeX parser' index.ts && touch docs/.nojekyll",
    "ncu": "ncu -u && pnpm i && git add package.json pnpm-lock.yaml && git commit -m ncu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retorquere/bibtex-parser.git"
  },
  "keywords": [
    "bibtex"
  ],
  "author": "Emiliano Heyns",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "csv": "^5.5.0",
    "digital-tree": "^2.0.2",
    "jest": "^26.6.3",
    "jest-specific-snapshot": "^4.0.0",
    "markdown": "^0.5.0",
    "mkdirp": "^1.0.4",
    "pegjs": "^0.10.0",
    "slugify": "^1.5.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "unicode2latex": "^2.1.35",
    "xregexp": "^5.0.2"
  },
  "jest": {
    "coverageDirectory": "node_modules/.cache/coverage",
    "collectCoverageFrom": [
      "*.ts",
      "!*.d.ts"
    ],
    "reporters": [
      "default"
    ],
    "testSequencer": "./test-sequencer.js"
  },
  "files": [
    "package.json",
    "grammar.js",
    "grammar.d.ts",
    "chunker.d.ts",
    "chunker.js",
    "index.d.ts",
    "index.js",
    "jabref.d.ts",
    "jabref.js",
    "README.md",
    "strings.json",
    "unabbrev.json",
    "abbrev.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
